<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.side.wiki.mapper.DebateBoardMapper">
	
	<select id="getAllDetail" resultType="com.side.wiki.vo.DebateReplyVO">
		select doc_title, email, content, regdate 
		from debatereply where doc_title = #{docTitle}
	</select>

</mapper>